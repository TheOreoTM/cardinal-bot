{
    "name": "cardinal-bot",
    "version": "1.6.0",
    "main": "./dist/index.js",
    "author": "@theoreotm",
    "license": "MIT",
    "type": "module",
    "imports": {
        "#config": "./dist/config.js",
        "#constants": "./dist/lib/util/constants.js",
        "#lib/*": "./dist/lib/*.js",
        "#lib/structures": "./dist/lib/structures/index.js",
        "#lib/discord": "./dist/lib/discord/index.js",
        "#lib/setup": "./dist/lib/setup/index.js",
        "#lib/types": "./dist/lib/types/index.js",
        "#lib/decorators": "./dist/lib/decorators/index.js",
        "#lib/database": "./dist/lib/database/index.js",
        "#lib/services": "./dist/lib/services/index.js",
        "#utils/*": "./dist/lib/util/*.js",
        "#utils/common": "./dist/lib/util/common/index.js",
        "#utils/functions": "./dist/lib/util/functions/index.js",
        "#root/*": "./dist/*.js",
        "@prisma/client/runtime/library": "@prisma/client/runtime/library.js"
    },
    "dependencies": {
        "@prisma/client": "^5.0.0",
        "@sapphire/async-queue": "^1.5.0",
        "@sapphire/decorators": "^6.0.1",
        "@sapphire/discord.js-utilities": "^7.0.1",
        "@sapphire/fetch": "^2.4.1",
        "@sapphire/framework": "^4.5.1",
        "@sapphire/pieces": "^3.7.0",
        "@sapphire/plugin-api": "^5.1.1",
        "@sapphire/plugin-editable-commands": "^3.0.2",
        "@sapphire/plugin-logger": "^3.0.4",
        "@sapphire/plugin-scheduled-tasks": "^8.0.0",
        "@sapphire/plugin-subcommands": "^4.0.1",
        "@sapphire/ratelimits": "^2.4.7",
        "@sapphire/shapeshift": "^3.9.6",
        "@sapphire/snowflake": "^3.5.1",
        "@sapphire/stopwatch": "^1.5.0",
        "@sapphire/time-utilities": "^1.7.10",
        "@sapphire/type": "^2.4.0",
        "@sapphire/utilities": "^3.13.0",
        "@sentry/cli": "^2.30.2",
        "@sentry/integrations": "^7.107.0",
        "@sentry/node": "^7.107.0",
        "@skyra/char": "^1.0.3",
        "@skyra/env-utilities": "^1.2.1",
        "@types/ioredis": "5.0.0",
        "bullmq": "^4.2.1",
        "colorette": "^2.0.20",
        "decancer": "^3.0.2",
        "discord-api-types": "^0.37.51",
        "discord.js": "^14.11.0",
        "enhanced-ms": "^2.3.0",
        "fuzzysort": "^2.0.4",
        "he": "^1.2.0",
        "ioredis": "5.3.2",
        "lexure": "^0.17.0",
        "prom-client": "^15.1.0",
        "random": "^4.1.0",
        "randomatic": "^3.1.1",
        "table": "^6.8.1",
        "tsup": "^7.1.0",
        "uuidv4": "^6.2.13"
    },
    "devDependencies": {
        "@sapphire/cli": "^1.6.1",
        "@sapphire/prettier-config": "^1.4.5",
        "@sapphire/ts-config": "^4.0.1",
        "@types/he": "^1",
        "@types/node": "^18.16.18",
        "@types/randomatic": "^3.1.3",
        "@types/ws": "^8.5.5",
        "prettier": "^2.8.8",
        "prisma": "^5.0.0",
        "tsc-watch": "^6.0.4",
        "typescript": "^5.1.3"
    },
    "resolutions": {
        "@sapphire/utilities": "3.16.1"
    },
    "scripts": {
        "sapphire": "sapphire",
        "generate": "sapphire generate",
        "build": "tsup && yarn sentry:sourcemaps",
        "watch": "tsup --watch --onSuccess \"node ./dist/index.js\"",
        "start": "node dist/index.js",
        "release": "cj",
        "preview": "cj --dry-run",
        "deploy": "yarn prisma:push-prod && yarn build && NODE_ENV=production yarn start",
        "dev": "yarn prisma:push-dev && yarn build && yarn start",
        "format": "prettier --write \"src/\"",
        "db:push": "npx prisma db push",
        "db:generate": "npx prisma generate",
        "prisma:push-dev": "dotenv -e .env.development -- npx prisma db push",
        "prisma:push-prod": "dotenv -e .env -- npx prisma db push",
        "prisma:generate": "npx prisma generate",
        "sentry:sourcemaps": "sentry-cli sourcemaps inject --org theoreotm --project cardinal ./dist && sentry-cli sourcemaps upload --org theoreotm --project cardinal ./dist"
    },
    "prettier": "@sapphire/prettier-config",
    "packageManager": "yarn@1.22.21"
}
