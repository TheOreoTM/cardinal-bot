		"db:generate": "npx prisma generate",
		"prisma:push-dev": "dotenv -e .env.development -- npx prisma db push",
		"prisma:push-prod": "dotenv -e .env -- npx prisma db push",
		"prisma:generate": "npx prisma generate"
